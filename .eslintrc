{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": "off",
    "camelcase": "off",
    "react/react-in-jsx-scope": "off",
    "spaced-comment": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      2
    ],
    "no-unused-vars": "warn"
  }
}